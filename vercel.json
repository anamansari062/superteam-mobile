{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "styles/**/*",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/styles/.*",
      "status": 404,
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "public/index.html"
    }
  ],
  "files": ["!styles/**/*.css"]
}
